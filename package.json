{
  "name": "ts-electron-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev:main": "electronmon src/main/dist/index.cjs src/preload/dist/index.cjs",
    "build:main": "tsc --project src/main/tsconfig.json",
    "watch:main": "tsc --watch --project src/main/tsconfig.json",
    "build:preload": "vite build --config src/preload/vite.config.ts",
    "watch:preload": "vite build --watch --config src/preload/vite.config.ts",
    "dev:renderer": "vite --config src/renderer/vite.config.ts",
    "build:renderer": "vite build --config src/renderer/vite.config.ts",
    "dev": "concurrently \"npm run watch\" \"npm run dev:renderer\" \"npm run dev:main\"",
    "watch": "concurrently \"npm run watch:main\" \"npm run watch:preload\"",
    "build": "npm run cleanup && npm run build:main && npm run build:preload && npm run build:renderer",
    "cleanup": "rimraf src/main/dist src/preload/dist src/renderer/dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "concurrently": "7.5.0",
    "electron": "21.3.0",
    "electron-builder": "23.6.0",
    "electronmon": "2.0.2",
    "rimraf": "3.0.2",
    "typescript": "4.9.3",
    "vite": "3.2.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
